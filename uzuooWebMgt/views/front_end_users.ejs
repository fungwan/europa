<div id="verified-dialogM" tabindex="-1" role="dialog" aria-hidden="true" class="modal fade">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header modal-header-primary">
                <button type="button" data-dismiss="modal" aria-hidden="true" class="close">&times;</button>
                <h4 class="modal-title">现场认证</h4>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <div class="input-icon right"><i class="fa fa-user"></i>
                                <input ng-model="selectMerchant.shop_name" type="text" value="商家名" placeholder="商家名" class="form-control" />
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <form class="form-horizontal">
                            <div class="form-body pal">
                                <div class="form-group">
                                    <div id="preview">
                                        <img id="imghead" border=1 src="{{selectMerchant.selectImg1}}" width="256" height="256" />
                                        <img id="imghead" border=1 src="{{selectMerchant.selectImg2}}" width="256" height="256" />
                                    </div>
                                </div>
                                <input id="input-upload" type="file" ng-file-select="onFileMultiSelect($files)" multiple accept="image/*"/>
                            </div>
                        </form>
                    </div>
                </div>
                <div class="row" style="height:150px; overflow:auto;">
                    <div class="panel panel-yellow">
                        <!--div class="panel-heading">认证记录</div-->
                        <div class="panel-body">
                            <table id="verifiedRecord-table" class="table table-hover table-bordered">
                                <thead>
                                <tr>
                                    <th>申请日期</th>
                                    <th>认证结果</th>
                                    <th>驳回理由</th>
                                </tr>
                                </thead>
                                <tbody>
                                <tr ng-repeat="verifiedLog in selectMerchant.verifiedLogsArray">
                                    <td ng-bind="timeToStr(verifiedLog.apply_time*1000)">2015/12/12</td>
                                    <td ng-bind="verifyStatus(verifiedLog.verification_state)">失败</td>
                                    <td  ng-bind="verifiedLog.description">无法看清号码</td>
                                </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
                <div class="form-actions text-right pal">
                    <button ng-click="verifiedMerchant()" data-dismiss="modal" class="btn btn-pink">通过认证</button>
                </div>
            </div>
        </div>
    </div>
</div>